{
  "name": "aargon-ui",
  "version": "1.0.0",
  "description": "A List of Animated Components for React Native",
  "license": "MIT",
  "author": "Md Muhaiminul",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web:build": "expo export -p web",
    "web:preview": "npx serve dist",
    "ts:check": "tsc",
    "lint": "eslint . --fix",
    "clean": "npx expo prebuild --clean",
    "update": "npx expo install --check",
    "build:ios": "eas build -p ios --profile production",
    "submit": "eas submit --platform ios",
    "format": "prettier --write \"*/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@animatereactnative/accordion": "^0.1.5",
    "@expo/metro-runtime": "~4.0.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@shopify/react-native-skia": "1.5.0",
    "expo": "~52.0.30",
    "expo-constants": "~17.0.5",
    "expo-splash-screen": "~0.29.21",
    "moti": "^0.30.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.8",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.7",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-uuid": "^2.0.3",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@types/color": "^4.2.0",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^9.19.0",
    "eslint-config-universe": "^14.0.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.3"
  },
  "private": true
}
